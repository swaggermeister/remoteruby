<p id="notice"><%= notice %></p>
<h3>Profile</h3>
<div class="form-group">
  <label class="form-label" for="name">Company Name</label>
  <input type="text" class="form-input col-6" id="name" placeholder="Company Name">
</div>
<div class="form-group">
  <label class="form-label" for="username">Username</label>
  <input class="form-input col-5" type="text" id="username" placeholder="Username">
</div>
<div class="form-group">
  <label class="form-label" for="password">Password</label>
  <input class="form-input col-5" type="password" id="password" placeholder="Password">
</div>
