<div class="job-listings-header my-2">
  <h3>Your Job Listings</h3>
</div>
<div class="my-job-listings">
  <% if @job_listings.empty? %>
    <h4>You don't have any listings yet!</h4>
    <%= link_to 'Start a listing now.', new_job_listing_path %>
  <% else %>
    <%= render @job_listings %>
  <% end %>
